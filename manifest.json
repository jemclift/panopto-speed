{

  "manifest_version": 2,
  "name": "Panopto Speed",
  "version": "1.0",

  "description": "Adjust Speed On Panopto",

  "homepage_url": "https://github.com/jemclift",

  "icons": {
    "128": "icons/icon-128.png",
    "48": "icons/icon-48.png",
    "16": "icons/icon-16.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.panopto.eu/*"],
      "css": ["style.css"],
      "js": ["script.js"]
    }
  ]

}
